#: ../../source/labs/lab_05_loopy_lab/loopy_lab.rst:68
msgid "You can count \"backwards\" by subtracting."
msgstr ""

#: ../../source/labs/lab_05_loopy_lab/loopy_lab.rst:70
msgid ""
"print(\"Count up!\")\n"
"for i in range(10):\n"
"    print(i)\n"
"\n"
"print()\n"
"print(\"Count down, even if i is going up\")\n"
"for i in range(10):\n"
"    x = 9 - i\n"
"    print(\"i is\", i, \"and 9-i is\", x)"
